<div class="rawio" id="rawio" data-network="{{network}}">
	<div class="output">
		{{#each data}}
			{{this}}<br>
		{{/each}}
	</div>
	<form class="input" data-event="rawio" data-event-type="submit">
		<input
			type="text"
			id="rawio-input"
			autocomplete="off"
			placeholder="Enter raw IRC command"
		>
	</div>
</div>
